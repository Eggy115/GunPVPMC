command /credits:
    trigger:
        set {_credits} to a new chest inventory with 6 row with name "&7Gun&4PVP&aMC&6 Credits"

        set {_e} to 0
        loop 54 times:
            set slot {_e} of {_credits} to gray stained glass pane named "&0 "    
            add 1 to {_e}

        set slot 10 of {_credits} to player head named "&fEggy115" with nbt "{SkullOwner:Eggy115}"
        set line 1 of lore of slot 10 of {_credits} to "&6Created server"
        set line 2 of lore of slot 10 of {_credits} to "&6All the code"

        set slot 11 of {_credits} to player head named "&f7Iron" with nbt "{SkullOwner:7Iron}"
        set line 1 of lore of slot 11 of {_credits} to "&6First/second player"
        set line 2 of lore of slot 11 of {_credits} to "&6Bug fixes"

        set slot 12 of {_credits} to player head named "&fbesitX" with nbt "{SkullOwner:besitX}"
        set line 1 of lore of slot 12 of {_credits} to "&6First/second player"

        set slot 13 of {_credits} to player head named "&fLazyyyyyyishhhhh" with nbt "{SkullOwner:Lazyyyyyyishhhhh}"
        set line 1 of lore of slot 13 of {_credits} to "&6Found most bugs out of anyone"
        set line 2 of lore of slot 13 of {_credits} to "&6Help with building"
        set line 3 of lore of slot 13 of {_credits} to "&6Many many suggestions"

        set slot 14 of {_credits} to player head named "&fKilledbytheTV" with nbt "{SkullOwner:KilledbytheTV}"
        set line 1 of lore of slot 14 of {_credits} to "&6Help with building OG map"
        set line 2 of lore of slot 14 of {_credits} to "&6Recorded OG map"

        set slot 15 of {_credits} to player head named "&fk0ekj3s_" with nbt "{SkullOwner:k0ekj3s_}"
        set line 1 of lore of slot 15 of {_credits} to "&6Help with building"
        set line 2 of lore of slot 15 of {_credits} to "&6Particularly decorating"
        set line 3 of lore of slot 15 of {_credits} to "&6Extensive documentation"

        set slot 16 of {_credits} to player head named "&funskye_" with nbt "{SkullOwner:unskye_}"
        set line 1 of lore of slot 16 of {_credits} to "&6Helper"
        set line 2 of lore of slot 16 of {_credits} to "&6Much time AFK"

        set slot 19 of {_credits} to player head named "&fTopTierWaffle" with nbt "{SkullOwner:TopTierWaffle}"
