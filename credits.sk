        set line 2 of lore of slot 19 of {_credits} to "&6Wrote 90%% of the wiki"

        set slot 20 of {_credits} to player head named "&fCoinCatcher" with nbt "{SkullOwner:CoinCatcher}"
        set line 1 of lore of slot 20 of {_credits} to "&6Ghillie suits"
        set line 2 of lore of slot 20 of {_credits} to "&6Forest area"

        set slot 21 of {_credits} to player head named "&fL4pinha" with nbt "{SkullOwner:L4pinha}"
        set line 1 of lore of slot 21 of {_credits} to "&6Broken guns"
        set line 2 of lore of slot 21 of {_credits} to "&6Bank notes"
        set line 3 of lore of slot 21 of {_credits} to "&6/trash"

        set slot 22 of {_credits} to player head named "&fbessthesigma" with nbt "{SkullOwner:bessthesigma}"
        set line 1 of lore of slot 22 of {_credits} to "&6Helped decorate"
        set line 2 of lore of slot 22 of {_credits} to "&6Secretary"

        set slot 23 of {_credits} to player head named "&fDragon04092" with nbt "{SkullOwner:Dragon04092}"
