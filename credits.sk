        set slot 28 of {_credits} to player head named "&fjay_carey124" with nbt "{SkullOwner:jay_carey124}"
        set line 1 of lore of slot 28 of {_credits} to "&6Building"       
        
        set slot 29 of {_credits} to player head named "&fxSetsumakix" with nbt "{SkullOwner:xSetsumakix}"
        set line 1 of lore of slot 29 of {_credits} to "&6SSK.950 Rifle"  
        set line 2 of lore of slot 29 of {_credits} to "&624.1x70mm"  
        
        set slot 30 of {_credits} to player head named "&fstardistroier" with nbt "{SkullOwner:stardistroier}"
        set line 1 of lore of slot 30 of {_credits} to "&6Tank stats and information"          

        open {_credits} to player
        set {trade::%player's uuid%} to true            